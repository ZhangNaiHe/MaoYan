<body>
  <!-- 头部 -->
<div class="header--mini">
  <div class="wrapper cf">
    <a class="site-logo" href=""></a>
  </div>
</div>


 <!-- 注册 -->
<div class="content">
    <div class="J-unitive-signup-form">
      <div class="sheet">
        
        <form action="" method="post">
          <!-- 手机号 -->
          <div class="form-field iphone">
            <label>手机号</label>
            <input class="f-text" type="number" value="">
            <span class="unitive-tip">注册成功后，全美团通用</span>
          </div>
          <!-- 获取短信动态码 -->
          <div class="form-field Dynamic">
            <input  class="btn-normal btn-mini verify-btn J-verify-btn" type="button" value="免费获取短信动态码">
          </div>
            <!-- 短信动态码 -->
        <div class="form-field  note">
            <label>短信动态码</label>
          <input class="f-text" type="number" name="" id="">
        </div>
        <!-- 创建密码 -->
        <div class=" form-field password">
          <label>创建密码</label>
          <input type="password" name="" class="f-text" >
        </div>
        <!-- 确认密码 -->
        <div class=" form-field affirm">
          <label>确认密码</label>
          <input class="f-text" type="password" name="" id="">
        </div>

        <!-- 点寂注册 -->
        <div class="form-field  submit">
          <input type="button" value="同意以下协议并注册">
        </div>
        </form>
      </div>
    </div>
    <div class="terms">
      <a href="">《美团用户协议》</a>
    </div>
  </div>


